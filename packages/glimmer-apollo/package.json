{
  "name": "glimmer-apollo",
  "version": "0.6.6",
  "description": "Ember and Glimmer integration for Apollo Client",
  "keywords": [
    "ember-addon",
    "glimmer",
    "apollo",
    "graphql"
  ],
  "repository": "http://github.com/josemarluedke/glimmer-apollo",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "files": [
    "dist",
    "README.md",
    "ember-addon-main.js",
    "package.json"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "typings": "dist/index.d.ts",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w",
    "test": "echo 'TODO'",
    "test:types": "tsc --noEmit",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.1",
    "@embroider/addon-shim": "^1.0.0",
    "@wry/equality": "^0.5.2"
  },
  "devDependencies": {
    "@apollo/client": "3.5.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.16.7",
    "@embroider/addon-dev": "^1.0.0",
    "graphql": "^16.3.0",
    "rollup": "^2.66.1",
    "rollup-plugin-ts": "^2.0.4",
    "typescript": "^4.5.5",
    "walk-sync": "^3.0.0"
  },
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "@glimmer/tracking": "^1.1.2 || ^2.0.0-beta.15",
    "ember-source": "^3.27.0 || ^4.0.0 || ^5.0.0 || ^6.0.0",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "peerDependenciesMeta": {
    "ember-source": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "ember-addon-main.js"
  },
  "volta": {
    "node": "14.17.0"
  }
}
