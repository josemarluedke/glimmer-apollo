{
  "name": "test-glimmerx",
  "version": "0.0.0",
  "private": true,
  "description": "A brand new Glimmer app.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "start": "webpack serve --mode development --env development",
    "test": "npm-run-all test:browsers",
