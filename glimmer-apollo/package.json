{
  "name": "glimmer-apollo",
  "version": "0.7.0",
  "description": "Ember and Glimmer integration for Apollo Client",
  "keywords": [
    "ember-addon"
  ],
  "repository": "http://github.com/josemarluedke/glimmer-apollo",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'"
  },
  "dependencies": {
    "@ember/test-waiters": "^4.1.1",
    "@embroider/addon-shim": "^1.10.2",
    "@wry/equality": "^0.5.2",
    "decorator-transforms": "^2.2.2"
  },
  "devDependencies": {
    "@apollo/client": "^4.0.0",
    "rxjs": "^7.0.0",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@embroider/addon-dev": "^8.1.2",
    "@eslint/js": "^9.39.2",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.7.3",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.12",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
    "ember-source": "^6.9.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "rollup": "^4.53.5",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "peerDependencies": {
    "@apollo/client": "^4.0.0",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "rxjs": "^7.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {}
  }
}
