{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for test-app goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite",
    "test": "concurrently \"pnpm:test:*\" --names \"test:\" --prefixColors auto",
    "test:ember": "vite build --mode test && ember test --path dist"
  },
  "devDependencies": {
    "@apollo/client": "^3.12.6",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@ember/optional-features": "^2.3.0",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/compat": "4.1.11",
    "@embroider/config-meta-loader": "1.0.0",
    "@embroider/core": "4.4.1",
    "@embroider/macros": "^1.19.5",
    "@embroider/test-setup": "^4.0.0",
    "@embroider/vite": "1.4.4",
    "@eslint/js": "^9.39.2",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "1.7.3",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.12",
    "@types/eslint__js": "^9.14.0",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "@types/sinon": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@warp-drive/core-types": "~5.8.0",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.2.2",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.9.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-route-template": "^1.0.3",
    "ember-source": "^6.9.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-qunit": "^8.2.5",
    "glimmer-apollo": "workspace:*",
    "globals": "^16.5.0",
    "loader.js": "^4.7.0",
    "msw": "^2.12.4",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "qunit": "^2.24.3",
    "qunit-dom": "^3.5.0",
    "sinon": "^21.0.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^5.0.3",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "pnpm": {
    "overrides": {
      "ember-source": "$ember-source"
    }
  }
}